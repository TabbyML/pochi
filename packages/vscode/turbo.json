{
  "tags": ["public"],
  "extends": ["//"],
  "$schema": "https://turborepo.com/schema.json",
  "boundaries": {
    "implicitDependencies": ["vscode", "mocha"]
  },
  "tasks": {
    "build": {
      "dependsOn": ["@getpochi/vscode-webui#build"],
      "outputs": ["assets/webview-ui/**", "dist/**"]
    },
    "test:coverage": {
      "outputs": ["coverage/**"]
    }
  }
}
